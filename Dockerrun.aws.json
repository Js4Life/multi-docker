{
    "AWSEBDockerrunVersion":2,
    "containerDefinitions":[
       {
          "name":"client",
          "image":"raghu1/multi-client",
          "hosname":"client",
          "essential":false,
          "memory":128
       },
       {
          "name":"server",
          "image":"raghu1/multi-server",
          "hosname":"api",
          "essential":false,
          "memory":128
       },
       {
          "name":"worker",
          "image":"raghu1/multi-worker",
          "hosname":"worker",
          "essential":false,
          "memory":128
       },
       {
          "name":"nginx",
          "image":"raghu1/multi-nginx",
          "hosname":"nginx",
          "essential":true,
          "memory":128,
          "portMappings":[
             {
                "hostPort":80,
                "containerPort":80
             }
          ],
          "links":[
             "client",
             "server"
          ]
       }
    ]
 }